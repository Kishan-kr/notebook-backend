{
  "version":2,
  "builds":[
    {
    "src":"./index.js",
    "use":"@vercel/node"
  }],
  "rewrites":[
    {
      "src":"/(.*)",
      "dest":"/"
    }
  ],
  "headers": [
        {
          "source": "/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        },
        {
          "source": "/notebook-backend-taupe.vercel.app//(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        }  
    ]
}
